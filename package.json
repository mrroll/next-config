{
  "name": "@mrroll/next-config",
  "version": "13.88.0",
  "license": "MIT",
  "engines": {
    "node": "^14.19.0 || ^16.10.0 || >=17.0.0"
  },
  "repository": "https://github.com/mrroll/next-config",
  "author": "mrroll",
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./*": "./lib/*.js"
  },
  "files": [
    "/lib/**/*"
  ],
  "scripts": {
    "build": "npm-run-all --sequential build:clean build:typescript build:alias",
    "build:alias": "tsc-alias --project build.tsconfig.json --verbose",
    "build:clean": "rimraf ./lib",
    "build:typescript": "cross-env NODE_ENV=production TZ=UTC tsc --project build.tsconfig.json",
    "dev": "npm-run-all --parallel dev:watch dev:build",
    "dev:build": "cross-env NODE_ENV=development TZ=UTC nodemon --config build.nodemon.json",
    "dev:watch": "cross-env NODE_ENV=development TZ=UTC TS_NODE_BASEURL=. nodemon --config nodemon.json ./src/dev.ts"
  },
  "volta": {
    "node": "18.14.0",
    "yarn": "1.22.19"
  },
  "dependencies": {
    "@svgr/webpack": "^6.5.1",
    "css-loader": "^6.7.3",
    "ts-deepmerge": "^6.0.2"
  },
  "devDependencies": {
    "@mrroll/eslint-config-typescript": "^0.102.0",
    "@swc/core": "^1.3.32",
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.11.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "next": "^13.1.6",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.5",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4",
    "webpack": "^5.75.0",
    "yalc": "^1.0.0-pre.53"
  }
}
